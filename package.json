{
  "packageManager": "pnpm@7.6.0",
  "scripts": {
    "dev": "pnpm -C packages/core run dev",
    "build": "pnpm -C packages/core run build",
    "docs:dev": "pnpm -C packages/docs run docs:dev",
    "docs:build": "pnpm -C packages/docs run docs:build",
    "docs:serve": "pnpm -C packages/docs run docs:serve",
    "clean": "rimraf dist packages/*/dist",
    "lint": "prettier --write .",
    "patch": "pnpm -C packages/core run patch",
    "minor": "pnpm -C packages/core run minor",
    "major": "pnpm -C packages/core run major"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.18",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.6",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vueuse/core": "^9.1.1",
    "less": "^4.1.3",
    "lodash": "^4.17.21",
    "manci-ui": "workspace:^0.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "unocss": "^0.49.1",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.15.2",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^3.0.8",
    "vitepress": "^1.0.0-alpha.4",
    "vue": "^3.2.37"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@interactjs/core",
        "@interactjs/utils"
      ]
    }
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.5.0",
    "@manc-ui/hooks": "workspace:*",
    "@manc-ui/theme": "workspace:^0.0.0"
  }
}
